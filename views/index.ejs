<h1 class="text-5xl mt-10 font-bold text-center">
    Ini adalah Halaman Utama
</h1>

<h2 class="text-3xl text-center">Halo <%= nama %> </h2>
<h4 class="text-xl text-center font-semibold">Daftar nama mahasiswa</h4>
<% if (mahasiswa.length < 1) { %>
<p class="text-xl text-red-500 text-center">Data Mahasiswa masih kosong</p>
<% } else { %>
<% mahasiswa.forEach(mhs => { %>
<ul class="text-slate-700 font-semibold">
    <li>Nama : <%= mhs.nama %> </li>
    <li>Email : <%= mhs.email %> </li>
</ul>
<% }) %>
<% } %>